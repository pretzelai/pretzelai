{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "strictNullChecks": true,
    "target": "ES2018"
  },
  "include": [
    "src/**/*"
  ],
  "references": [
    {
      "path": "../application"
    },
    {
      "path": "../apputils"
    },
    {
      "path": "../notebook"
    },
    {
      "path": "../services"
    },
    {
      "path": "../settingregistry"
    }
  ],
  "extends": "../../tsconfigbase"
}
